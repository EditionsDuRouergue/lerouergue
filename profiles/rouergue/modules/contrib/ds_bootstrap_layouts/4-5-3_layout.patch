diff --git a/profiles/rouergue/modules/contrib/ds_bootstrap_layouts/ds_bootstrap_layouts.module b/profiles/rouergue/modules/contrib/ds_bootstrap_layouts/ds_bootstrap_layouts.module
index 15a1237..d355843 100644
--- a/profiles/rouergue/modules/contrib/ds_bootstrap_layouts/ds_bootstrap_layouts.module
+++ b/profiles/rouergue/modules/contrib/ds_bootstrap_layouts/ds_bootstrap_layouts.module
@@ -235,5 +235,16 @@ function ds_bootstrap_layouts_ds_layout_info() {
     'image' => TRUE,
   );
 
+  $layouts['bootstrap_4_5_3'] = array(
+    'label' => t('Bootstrap Three Columns: 4-5-3'),
+    'path' => $threecol . '/bootstrap_4_5_3',
+    'regions' => array(
+      'left' => t('Left'),
+      'central' => t('Middle'),
+      'right' => t('Right'),
+    ),
+    'form' => TRUE,
+    'image' => TRUE,
+  );
   return $layouts;
 }
\ No newline at end of file
diff --git a/profiles/rouergue/modules/contrib/ds_bootstrap_layouts/layouts/threecol/bootstrap_4_5_3/bootstrap-4-5-3.tpl.php b/profiles/rouergue/modules/contrib/ds_bootstrap_layouts/layouts/threecol/bootstrap_4_5_3/bootstrap-4-5-3.tpl.php
new file mode 100644
index 0000000..167d42e
--- /dev/null
+++ b/profiles/rouergue/modules/contrib/ds_bootstrap_layouts/layouts/threecol/bootstrap_4_5_3/bootstrap-4-5-3.tpl.php
@@ -0,0 +1,30 @@
+<?php
+/**
+ * @file
+ * Bootstrap 4-5-3 template for Display Suite.
+ */
+?>
+
+
+<<?php print $layout_wrapper; print $layout_attributes; ?> class="<?php print $classes; ?>">
+  <?php if (isset($title_suffix['contextual_links'])): ?>
+    <?php print render($title_suffix['contextual_links']); ?>
+  <?php endif; ?>
+  <div class="row">
+    <<?php print $left_wrapper; ?> class="col-sm-4 <?php print $left_classes; ?>">
+      <?php print $left; ?>
+    </<?php print $left_wrapper; ?>>
+    <<?php print $central_wrapper; ?> class="col-sm-5 <?php print $central_classes; ?>">
+      <?php print $central; ?>
+    </<?php print $central_wrapper; ?>>
+    <<?php print $right_wrapper; ?> class="col-sm-3 <?php print $right_classes; ?>">
+      <?php print $right; ?>
+    </<?php print $right_wrapper; ?>>
+  </div>
+</<?php print $layout_wrapper ?>>
+
+
+<!-- Needed to activate display suite support on forms -->
+<?php if (!empty($drupal_render_children)): ?>
+  <?php print $drupal_render_children ?>
+<?php endif; ?>
